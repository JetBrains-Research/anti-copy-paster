<idea-plugin>
    <id>ru.hse.kirilenko.AntiCopyPaster.AntiCopyPaster</id>
    <name>AntiCopyPaster</name>
    <vendor email="kirilenko.haa@gmail.com" url="http://www.google.com">HSE</vendor>
    <idea-version since-build="3000"/>
    <description><![CDATA[
    Informs about possible refactorings on paste actions
    ]]></description>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>
    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <copyPastePreProcessor implementation="ExtractMethodPreProcessor"/>
    </extensions>
</idea-plugin>